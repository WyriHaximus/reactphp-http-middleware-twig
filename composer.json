{
    "name": "wyrihaximus/react-http-middleware-twig",
    "description": "ReactPHP HTTP Twig Middleware",
    "license": "MIT",
    "require": {
        "php": "^8 || ^7.4",
        "psr/http-message": "^1.0.1",
        "react/promise": "^2.8",
        "ringcentral/psr7": "^1.3.0",
        "twig/twig": "^3.3.10"
    },
    "require-dev": {
        "wyrihaximus/async-test-utilities": "^4.2.0"
    },
    "config": {
        "platform": {
            "php": "7.4.7"
        },
        "sort-packages": true
    },
    "autoload": {
        "psr-4": {
            "WyriHaximus\\React\\Http\\Middleware\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "WyriHaximus\\React\\Tests\\Http\\Middleware\\": "tests/"
        }
    },
    "scripts": {
        "post-install-cmd": [
            "composer normalize"
        ],
        "post-update-cmd": [
            "composer normalize"
        ]
    }
}
